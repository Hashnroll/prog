#include <iostream>
#include <map>
#include <string>
#include "graph.h"

using namespace std;

int main() {
	graph g1, g2;

	g1.input();
	g2.input();

	cout << "Graph g1: " << endl;
	g1.print();

	cout << "Graph g2: " << endl;
	g2.print();

	graph g3;
	g3 = inter_(g1, g2);

	cout << "Graph intersection: " << endl;
	g3.print();

	graph g4;
	g4 = union_(g1, g2);

	cout << "Graph union: " << endl;
	g4.print();

	system("pause");
  return 0;
}
